/* errVERIFY : 0x07 */
#define DFU_STATUS_ERR_VERIFY		0x07
/* errADDRESS : 0x08 */
#define DFU_STATUS_ERR_ADDRESS		0x08
/* errNOTDONE : 0x09 */
#define DFU_STATUS_ERR_NOTDONE		0x09
/* errFIRMWARE : 0x0A */
#define DFU_STATUS_ERR_FIRMWARE		0x0A




